<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <ImageView
        android:id="@+id/iv_main_background"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/iv_main_bk1"
        app:layout_constraintBottom_toBottomOf="@+id/iv_main_bk9"
        app:layout_constraintLeft_toLeftOf="@+id/iv_main_bk1"
        app:layout_constraintRight_toRightOf="@+id/iv_main_bk9"
        />
    <ImageView
        android:id="@+id/iv_main_background1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/iv_main_bk1"
        app:layout_constraintBottom_toBottomOf="@+id/iv_main_bk1"
        app:layout_constraintLeft_toLeftOf="@+id/iv_main_bk1"
        app:layout_constraintRight_toRightOf="@+id/iv_main_bk3"
        />
    <ImageView
        android:id="@+id/iv_main_background2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/iv_main_bk4"
        app:layout_constraintBottom_toBottomOf="@+id/iv_main_bk4"
        app:layout_constraintLeft_toLeftOf="@+id/iv_main_bk1"
        app:layout_constraintRight_toRightOf="@+id/iv_main_bk3"
        />
    <ImageView
        android:id="@+id/iv_main_background3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/iv_main_bk7"
        app:layout_constraintBottom_toBottomOf="@+id/iv_main_bk7"
        app:layout_constraintLeft_toLeftOf="@+id/iv_main_bk1"
        app:layout_constraintRight_toRightOf="@+id/iv_main_bk3"
        />
    <ImageView
        android:id="@+id/iv_main_background4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/iv_main_bk1"
        app:layout_constraintBottom_toBottomOf="@+id/iv_main_bk7"
        app:layout_constraintLeft_toLeftOf="@+id/iv_main_bk1"
        app:layout_constraintRight_toRightOf="@+id/iv_main_bk1"
        />
    <ImageView
        android:id="@+id/iv_main_background5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/iv_main_bk1"
        app:layout_constraintBottom_toBottomOf="@+id/iv_main_bk7"
        app:layout_constraintLeft_toLeftOf="@+id/iv_main_bk2"
        app:layout_constraintRight_toRightOf="@+id/iv_main_bk2"
        />
    <ImageView
        android:id="@+id/iv_main_background6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/iv_main_bk1"
        app:layout_constraintBottom_toBottomOf="@+id/iv_main_bk7"
        app:layout_constraintLeft_toLeftOf="@+id/iv_main_bk3"
        app:layout_constraintRight_toRightOf="@+id/iv_main_bk3"
        />

    <ImageButton
        android:id="@+id/ib_main_score"
        android:layout_width="@dimen/dimen_240"
        android:layout_height="@dimen/dimen_80"
        android:src="@drawable/button_scores"
        android:scaleType="centerInside"
        android:background="#00000000"
        android:layout_marginTop="@dimen/dimen_main_bt_margin"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />
    <TextView
        android:id="@+id/tv_main_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/score_text"
        android:textSize="50sp"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary"
        android:layout_marginBottom="@dimen/dimen_main_bt_margin"
        app:layout_constraintTop_toBottomOf="@id/ib_main_score"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />
    <ImageSwitcher
        android:id="@+id/is_main_1"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk1"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk1"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk1"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk1"
        />
    <ImageSwitcher
        android:id="@+id/is_main_2"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk2"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk2"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk2"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk2"
        />
    <ImageSwitcher
        android:id="@+id/is_main_3"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk3"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk3"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk3"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk3"
        />
    <ImageSwitcher
        android:id="@+id/is_main_4"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk4"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk4"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk4"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk4"
        />
    <ImageSwitcher
        android:id="@+id/is_main_5"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk5"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk5"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk5"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk5"
        />
    <ImageSwitcher
        android:id="@+id/is_main_6"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk6"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk6"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk6"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk6"
        />
    <ImageSwitcher
        android:id="@+id/is_main_7"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk7"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk7"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk7"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk7"
        />
    <ImageSwitcher
        android:id="@+id/is_main_8"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk8"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk8"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk8"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk8"
        />
    <ImageSwitcher
        android:id="@+id/is_main_9"
        android:layout_width="@dimen/dimen_button_size_medium"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:inAnimation="@anim/nav_default_enter_anim"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk9"
        app:layout_constraintLeft_toLeftOf="@id/iv_main_bk9"
        app:layout_constraintRight_toRightOf="@id/iv_main_bk9"
        app:layout_constraintBottom_toBottomOf="@id/iv_main_bk9"
        />
    
    <ImageView
        android:id="@+id/iv_main_bk1"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:layout_marginTop="@dimen/dimen_main_bt_margin_20"
        android:src="@drawable/button_bk"
        app:layout_constraintTop_toBottomOf="@id/tv_main_score"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/iv_main_bk2"/>

    <ImageView
        android:id="@+id/iv_main_bk2"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk1"
        app:layout_constraintLeft_toRightOf="@id/iv_main_bk1"
        app:layout_constraintRight_toLeftOf="@id/iv_main_bk3"/>

    <ImageView
        android:id="@+id/iv_main_bk3"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk1"
        app:layout_constraintLeft_toRightOf="@id/iv_main_bk2"
        app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
        android:id="@+id/iv_main_bk4"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"
        android:layout_marginTop="@dimen/dimen_main_bt_margin"
        app:layout_constraintTop_toBottomOf="@id/iv_main_bk1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/iv_main_bk2"/>

    <ImageView
        android:id="@+id/iv_main_bk5"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk4"
        app:layout_constraintLeft_toRightOf="@id/iv_main_bk1"
        app:layout_constraintRight_toLeftOf="@id/iv_main_bk3"/>

    <ImageView
        android:id="@+id/iv_main_bk6"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"
        app:layout_constraintTop_toTopOf="@id/iv_main_bk4"
        app:layout_constraintLeft_toRightOf="@id/iv_main_bk2"
        app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
        android:id="@+id/iv_main_bk7"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"

        android:layout_marginTop="@dimen/dimen_main_bt_margin"
        app:layout_constraintTop_toBottomOf="@id/iv_main_bk4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/iv_main_bk2"/>

    <ImageView
        android:id="@+id/iv_main_bk8"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"

        app:layout_constraintTop_toTopOf="@id/iv_main_bk7"
        app:layout_constraintLeft_toRightOf="@id/iv_main_bk1"
        app:layout_constraintRight_toLeftOf="@id/iv_main_bk3"/>

    <ImageView
        android:id="@+id/iv_main_bk9"
        android:layout_width="@dimen/dimen_button_size"
        android:layout_height="@dimen/dimen_button_size"
        android:src="@drawable/button_bk"

        app:layout_constraintTop_toTopOf="@id/iv_main_bk7"
        app:layout_constraintLeft_toRightOf="@id/iv_main_bk2"
        app:layout_constraintRight_toRightOf="parent"/>
    
    <ImageButton
        android:id="@+id/ib_main_reset"
        android:layout_width="@dimen/dimen_button_size_large"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:src="@drawable/button_reset"
        android:scaleType="centerInside"
        android:background="#00000000"
        android:layout_marginTop="@dimen/dimen_main_bt_margin_50"
        app:layout_constraintTop_toBottomOf="@id/iv_main_bk7"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/ib_main_clear"
        />
    <ImageButton
        android:id="@+id/ib_main_clear"
        android:layout_width="@dimen/dimen_button_size_large"
        android:layout_height="@dimen/dimen_button_size_medium"
        android:src="@drawable/button_clear"
        android:scaleType="centerInside"
        android:background="#00000000"
        app:layout_constraintTop_toTopOf="@id/ib_main_reset"
        app:layout_constraintLeft_toRightOf="@id/ib_main_reset"
        app:layout_constraintRight_toRightOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>